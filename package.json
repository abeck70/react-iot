{
  "name": "react-iot",
  "version": "0.0.1",
  "description": "React-IoT is a React renderer for Johny Five JavaScript framework",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack -p",
    "dev": "webpack-dev-server --history-api-fallback --content-base ./example --open --hot",
    "test": "mocha --require babel-register --reporter spec",
    "cover": "node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec --require babel-register tests/*"
  },
  "engines": {
    "node": ">=7.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnovick/React-IoT.git"
  },
  "keywords": [
    "React",
    "IoT",
    "Johny-five"
  ],
  "author": "Vladimir Novick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vnovick/React-IoT/issues"
  },
  "homepage": "https://github.com/vnovick/React-IoT#readme",
  "devDependencies": {
    "babel-istanbul": "^0.12.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^7.0.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
